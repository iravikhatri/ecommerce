    <!-- Product Gallery -->

    <!-- Product Gallery Design 1 -->
    <template id="product_gallery_design_one" name="Product Gallery Design 1">
        <section class="product_gallery_design_one custom pb-5">
            <div class="container">
                <div class="row p-2">
                    <t t-value="request.env['product.template'].sudo().search([('is_gallery_product', '=', True)])" t-set="products_object" />
                    <t t-set="number" t-value="int(0)" />
                    
                    <t t-foreach="products_object" t-as="product_object">
                        <t t-set="number" t-value="int(number) + 1" />

                        <t t-if="number &lt; 9">
                            <div class="col-lg-3 col-md-3 col-sm-3 col-6 zero_padding">
                                <a t-attf-href="{{'/shop/product/' + (product_object.name.lower()).replace(' ', '-') + '-' + str(product_object.id)}}">
                                    <div class="product_card_one">
                                        <div class="product_card_one-container">
                                            <div class="product_card_one-image">
                                                <span t-field="product_object.image" t-options="{'widget': 'image', 'resize': '300x300'}"/>
                                            </div>
                                            <div class="product_card_one-overlay">
                                                <span class="text-center product_card_one-see_details">See Details</span>
                                            </div>
                                        </div>
                                        <div class="product_card_one-content">
                                            <span class="pt-2 d-block product_card_one-name"><t t-esc="product_object.name" /></span>
                                            <span class=" d-block"><t t-esc="product_object.list_price"  t-options='{"widget": "monetary", "display_currency": product_object.currency_id}' /></span>
                                        </div>
                                    </div>
                                </a>
                            </div>
                        </t>
                    </t>
                </div>
                <div class="d-block p-4 mt-5">
                    <center>
                        <a class="button-shop_all text-center" href="/shop">See All Products</a>
                    </center>
                </div>
            </div>
        </section>
    </template>
